{
    "name": "xing/eway-api",
    "description": "eWAY PHP library",
    "keywords": [
        "eway",
        "gateway",
        "merchant",
        "pay",
        "payment",
        "commerce",
        "drupal",
        "xing"
    ],
    "type": "library",
    "homepage": "http://xing.net.au/eway",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jason Guo",
            "email": "jason.guo@xing.net.au",
            "homepage": "http://xing.net.au/",
            "role": "Developer"
        },
        {
            "name": "Joseph Zhao",
            "email": "joseph.zhao@xing.net.au",
            "homepage": "http://xing.net.au/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Eway\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eway\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=PSR2 src"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
